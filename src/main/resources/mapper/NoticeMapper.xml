<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wyu.xjxy.dao.NoticeDao">
    <insert id="insertNotice" parameterType="com.wyu.xjxy.entity.Notice">
		insert into notice(noticename,title,time,stuname,content)
		values(#{noticename},#{title},#{time},#{stuname},#{content});
</insert>
</mapper>